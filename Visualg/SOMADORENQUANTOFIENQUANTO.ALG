algoritmo "SomadorEnquantoFiEnquanto"
var
   N: Inteiro
   C: Inteiro
   S: Inteiro
inicio
      C <- 1
      S <- 0
      Enquanto (C<=5)faca
         escreval ("Digite um valor: ")
         leia (N)
         S <- S + N
         C <- C + 1
      FimEnquanto
      Escreval ("A soma dos valores e " , S)
fimalgoritmo